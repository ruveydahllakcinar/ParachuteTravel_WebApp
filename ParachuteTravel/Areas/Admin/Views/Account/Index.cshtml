@model AccountViewModel
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="col-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Edit Guide</h4>

            <form class="forms-sample" method="post" enctype="multipart/form-data">
                <div class="form-group">
                    <label for="exampleInputName1">Sender Id</label>
                    <input type="text" asp-for="SenderId" class="form-control" id="exampleInputName1" placeholder="SenderId">
                </div>
                <div class="form-group">
                    <label for="exampleInputName1">Receiver Id</label>
                    <input type="text" asp-for="ReceiverId" class="form-control" id="exampleInputName1" placeholder="ReceiverId">
                </div>
                <div class="form-group">
                    <label for="exampleInputName1">Ammount</label>
                    <input type="text" asp-for="Amount" class="form-control" id="exampleInputName1" placeholder="Ammount">
                </div>
                
                <div class="btn-sm mt-2">
                    <button type="submit" class="btn btn-primary">Transfer to Account</button>
                </div>
            </form>

        </div>
    </div>
</div>


