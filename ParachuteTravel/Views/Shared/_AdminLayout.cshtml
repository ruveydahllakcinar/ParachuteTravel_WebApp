
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en" class="light-style layout-menu-fixed" dir="ltr" data-theme="theme-default" data-assets-path="~/TraversalTheme_Admin/assets/" data-template="vertical-menu-template-free">

    @await Component.InvokeAsync("_AdminHeadPartial")
<body>
    <div class="layout-wrapper layout-content-navbar">
        <div class="layout-container">
            @await Component.InvokeAsync("_AdminSidebarPartial")
            <div class="layout-page">
                @await Component.InvokeAsync("_AdminNavbarPartial")
                <div class="content-wrapper">
                    <div class="container-xxl flex-grow-1 container-p-y">

                        @RenderBody()
                    </div>
                    @await Component.InvokeAsync("_AdminFooterPartial")
                </div>
            </div>
        </div>

        <!-- Overlay -->
        <div class="layout-overlay layout-menu-toggle"></div>
    </div>
    @await Component.InvokeAsync("_AdminScriptPartial")
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
